# Trough Production Environment Variables
# Copy this file to .env.production and fill in the values

# Database
POSTGRES_USER=trough
POSTGRES_PASSWORD=your-secure-password-here
POSTGRES_DB=trough

# API
API_PORT=8080
LOG_LEVEL=info

# Web
WEB_PORT=3000
PUBLIC_API_URL=https://your-domain.com/api

# Nginx (optional)
NGINX_HTTP_PORT=80
NGINX_HTTPS_PORT=443

# Google Maps API Key (for map view)
PUBLIC_GOOGLE_MAPS_API_KEY=your-google-maps-api-key
